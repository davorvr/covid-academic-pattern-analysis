@ARTICLE{Cao2004840,
author={Cao, L. and Wang, T.Y. and Chen, H.Z. and Qian, Y. and Chen, B.W. and Fang, P. and Sun, Y.X. and Zhu, R.N. and Deng, J. and Zhao, L.Q. and Mi, J. and Zhang, T.},
title={A preliminary investigation on the serological and epidemiological characteristics of severe acute respiratory syndrome in children},

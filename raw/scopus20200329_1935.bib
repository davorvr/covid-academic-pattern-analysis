@ARTICLE{Luo2020,
author={Luo, H. and Tang, Q.-L. and Shang, Y.-X. and Liang, S.-B. and Yang, M. and Robinson, N. and Liu, J.-P.},
title={Can Chinese Medicine Be Used for Prevention of Corona Virus Disease 2019 (COVID-19)? A Review of Historical Classics, Research Evidence and Current Prevention Programs},
journal={Chinese Journal of Integrative Medicine},
